<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}{% endblock %}</title>

    {#Required Open Graph metadata#}
    <meta property="og:title" content="{% block ogtitle %}{% endblock %}"/>
    <meta property="og:type" content="website"/>
    <meta property="og:url" content="{{ app.request.uri }}"/>
    <meta property="og:image" content="{% block ogimage %}{% endblock %}"/>
    {#Optional Open Graph metadata#}
    <meta property="og:image:width" content="{% block ogimagew %}1200{% endblock %}"/>
    <meta property="og:image:height" content="{% block ogimageh %}630{% endblock %}"/>
    <meta property="og:locale" content="{{ localeTerritory(app.request.locale) }}"/>
    <meta property="og:description" content="{% block ogdescription %}{% endblock %}"/>
    <meta property="og:determiner" content="{% block ogdeterminer %}{% endblock %}"/>
    <meta property="og:audio" content="{% block ogaudio %}{% endblock %}"/>

    {% block stylesheets %}{% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
